  <ItemGroup>
    <PackageReference Include="Colorspace" Version="1.0.2" />
    <PackageReference Include="Jint" Version="3.0.0-beta-2037" />
    <PackageReference Include="morelinq" Version="3.3.2" />
  </ItemGroup>

</Project>
